{
  "name": "@appfocused/ui-components",
  "version": "0.0.0",
  "main": "dist/index.js",
  "module": "dist/es/index.js",
  "types": "dist/es",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.8.2",
    "react-dom": "^16.8.2"
  },
  "devDependencies": {
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "@appfocused/eslint-config": "^0.0.0",
    "@appfocused/build-scripts": "^0.0.0",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "3.2.1",
    "@babel/cli": "^7.0.0",
    "@babel/core": "7.3.3",
    "@babel/plugin-proposal-class-properties": "7.3.3",
    "@babel/plugin-proposal-object-rest-spread": "7.3.2",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "babel-plugin-module-resolver": "3.2.0",
    "cross-env": "5.2.0"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn clean && yarn build:cjs && yarn build:esm && yarn build:es && yarn emit-types:es",
    "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel -s --source-maps --extensions '.ts,.tsx' ./src --out-dir ./dist --copy-files --ignore \"**/*.d.ts\"",
    "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel -s --source-maps --extensions '.ts,.tsx' ./src --out-dir ./dist/esm --copy-files --ignore \"**/*.d.ts\"",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel -s --source-map --extensions '.ts,.tsx' ./src --out-dir ./dist/es --copy-files --ignore \"**/*.d.ts\"",
    "emit-types:es": "tsc --declaration --emitDeclarationOnly --listEmittedFiles --jsx react --outDir dist/es",
    "emit-types:esm": "tsc --declaration --emitDeclarationOnly --listEmittedFiles --jsx react --outDir dist/esm"
  }
}
